{
    "compilerOptions": {
        "allowJs": true,
        "target": "es2018",
        "module": "commonjs",
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "strict": true,
        "removeComments": true,
        "skipLibCheck": true,
        "skipDefaultLibCheck": true
    }
}
